{
  "name": "Rocket Note",
  "version": "0.1.3",
  "author": "Rocket Note LLC",
  "manifest_version": 2,
  "description": "Extend the possiblities of YouTube with note-taking in a side panel, pins, and timestamps.",
  "homepage_url": "https://getrocketnote.com",
  "icons": {
	"16": "icons/icon16.png",
	"48": "icons/icon48.png",
	"128": "icons/icon128.png"
  },
  "default_locale": "en",
  "web_accessible_resources": [
	"assets/img/thumbtack_light.svg",
	"assets/img/thumbtack_dark.svg",
	"assets/img/rocket_note_main_logo.svg"
  ],
  "permissions": [
	"storage"
  ],
  "background": {
	"scripts": [
	  "src/bg/background.js"
	],
	"persistent": true
  },
  "browser_action": {
	"default_icon": "icons/icon16.png",
	"default_title": "Rocket Note",
	"default_popup": "src/browser_action/browser_action.html"
  },
  "content_scripts": [
	{
	  "matches": [
		"https://www.youtube.com/*"
	  ],
	  "css": [
		"src/inject/inject.css"
	  ]
	},
	{
	  "matches": [
		"https://www.youtube.com/*"
	  ],
	  "js": [
		"js/lib/jquery/jquery.js",
		"js/lib/moment/moment.min.js",
		"src/utils/utils.js",
		"src/requests/requests.js",
		"src/inject/inject.js",
		"src/inject/playlist.js"
	  ]
	}
  ]
}